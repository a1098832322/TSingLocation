package com.dsw.bean;

/**
 * 位置坐标的javaBean
 * 
 * @author 郑龙
 *
 */
public class Point {
	private float x, y;
	private String sbId;// 该位置绑定的设备ID

	public Point() {

	}

	public Point(float x, float y) {
		this.x = x;
		this.y = y;
	}

	public Point(float x, float y, String i) {
		this.x = x;
		this.y = y;
		this.sbId = i;
	}

	public float getX() {
		return x;
	}

	public void setX(float x) {
		this.x = x;
	}

	public float getY() {
		return y;
	}

	public void setY(float y) {
		this.y = y;
	}

	public void setPoint(float x, float y) {
		this.x = x;
		this.y = y;
	}

	/*
	 * (non-Javadoc)
	 * 
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		//log显示用的
		return "(" + x + "," + y + ")";
	}

	public String getSbId() {
		return sbId;
	}

	public void setSbId(String sbId) {
		this.sbId = sbId;
	}

}
